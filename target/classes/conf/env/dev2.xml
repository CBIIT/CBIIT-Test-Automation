<?xml version="1.0" encoding="UTF-8"?>
<environment>

	<profile-name>automation</profile-name>

	<application id="login">
		<login_url>https://service-dev2.nci.nih.gov/nih-covid-19-dashboard</login_url>
		<role name="Role">
			<user id="regular">
				<name>regular</name>
				<userId></userId><!-- add dev encrypted user -->
				<password></password><!-- add dev encrypted pass -->
			</user>
			<user id="Admin">
				<name></name>
				<userId></userId>
				<password></password>
			</user>

		</role>

		<db>
			<default>
				<driverclassname></driverclassname>
				<url></url>
				<username></username>
				<password></password>
			</default>
		</db>

		<native>
			<view>
				<native_view_url>https://service-dev2.nci.nih.gov/ncisp</native_view_url><!-- Add native view url -->
			</view>
		</native>
		
	</application>

</environment>
